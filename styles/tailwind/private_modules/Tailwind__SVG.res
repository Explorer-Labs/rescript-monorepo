open CssJs

// NOTE: Fill - Utilities for styling the fill of SVG elements.
let fillCurrent = style(.[SVG.fill(#currentColor)])

// NOTE: Stroke - Utilities for styling the stroke of SVG elements.
let strokeCurrent = style(.[SVG.stroke(#currentColor)])

// NOTE: Stroke Width - Utilities for styling the stroke width of SVG elements.
let stroke0 = style(.[SVG.strokeWidth(#px(0))])
let stroke1 = style(.[SVG.strokeWidth(#px(1))])
let stroke2 = style(.[SVG.strokeWidth(#px(2))])
